Host github.com
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_tony

Host *
	IdentityFile          ~/.ssh/id_tony
	Port                  22
	PreferredAuthentications  publickey,password
	ServerAliveCountMax		30
	ServerAliveInterval		120
	User                  ttp 
  AddKeysToAgent        yes
  UseKeychain           yes
  SendEnv               XMODIFIERS 
